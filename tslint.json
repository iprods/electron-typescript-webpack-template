{
  "extends": [
    "tslint:recommended",
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-floating-promises": false,
    "indent": [
      true,
      "spaces"
    ],
    "ter-indent": [
      true,
      2
    ],
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "ignore"
      }
    ]
  }
}
